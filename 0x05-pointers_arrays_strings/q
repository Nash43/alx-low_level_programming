#include <main.c>

/**                                                                                                                     
 * reset    _to_98 - takes a pointer to an int as a parameter and points it to 98                                              
 *                                                                   
 * @n: an inte      ger input pointer                                                                                                                                                                                                                                                                                        :         w       q          
 *                                                                                                         
 * Return     : nothing
 */      
    
void reset_to_98(int *n)      
{                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
	*n = 98;     
                                              }
